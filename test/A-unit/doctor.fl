program {
    string ans;

    writeln("Вы: Доктор, у меня недосып!");
    writeln("Доктор: Так давайте я Вам досыплю!");
    writeln("*сып-сып*");
    
    while(true) {
        writeln("Доктор: Ещё? (Да/Нет)");
        read(ans);

        if (ans == "Нет")
            break;
        else if (ans == "Да")
            writeln("*сып-сып*");
        else
            writeln("Доктор: Я Вас не понял!");
    }

    writeln("Доктор: Приходите ещё!");
}
