program {
    int a = 5;

    while (a > 0) {
        if (a >= 5)
            writeln(a, " конфеток осталось...");
        else if (a > 1)
            writeln(a, " конфетки осталось...");
        else
            writeln(a, " конфетка осталась...");
        a = a - 1;
    }

    writeln("Конфеток не осталось!");
}
