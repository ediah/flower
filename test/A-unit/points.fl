struct point {
    int x, y;
    string name;
}

program {
    struct point A, B;

    /* Структуры можно вводить отдельными полями */
    A.x = 0;
    A.y = 1;
    A.name = "A";
    
    /* А можно и целиком (при этом любое поле
                             может отсутствовать)*/
    B = {x = 2, name = "B", y = 3};

    writeln("Точки:");
    writeln(A.name, ": ", A.x, " ", A.y);
    writeln(B.name, ": ", B.x, " ", B.y);
}